    <DataSet Name="clients">
      <Query>
        <DataSourceName>DataSourceExpFinance</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@entite">
            <Value>=Parameters!entite.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct code_clien,NOM from AVOIR_FINANCIER af
join AF_AVOIR_GROUPE ag on af.numAvoir=ag.numAvoir
left
join GROUPE_FACTURE gf on ag.IDG=gf.ID_GROUPE_FACT and ag.Libre=0
left join AF_GROUPE_FACTURE agf on ag.IDG=agf.ID_GROUPE_FACT and ag.Libre=1 
left join AF_EF_GROUPE_FACTURE aegf on aegf.ID_GROUPE_FACT=ag.IDG and ag.Libre=3
join AF_CLIENT c on ISNULL(gf.CODE_CLIENT,ISNULL(agf.CODE_CLIENT,aegf.CODE_CLIENT))=c.code_clien and c.entite=@entite</CommandText>
      </Query>
      <Fields>
        <Field Name="code_clien">
          <DataField>code_clien</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="nom">
          <DataField>NOM</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="promotions">
      <Query>
        <DataSourceName>DataSourceExpFinance</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@dated">
            <Value>=Parameters!dated.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@datef">
            <Value>=Parameters!datef.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT  NOM_PROMOTION
FROM            AF_BORDEREAU
WHERE        (DATE_DOSSIER &gt;= @dated)  and (DATE_DOSSIER&lt;=@datef)
UNION ALL
SELECT 'AVOIR LIBRE'
UNION ALL
SELECT 'ESCOMPTE FINANCIER'</CommandText>
      </Query>
      <Fields>
        <Field Name="NOM_PROMOTION">
          <DataField>NOM_PROMOTION</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
