    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSourceExpFinance</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@entite">
            <Value>=Parameters!entite.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dated">
            <Value>=Parameters!dated.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@datef">
            <Value>=Parameters!datef.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@client">
            <Value>=Parameters!client.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@promotion">
            <Value>=Parameters!promotion.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@etat">
            <Value>=Parameters!etat.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT [ID_GROUPE_FACT]
      ,[code_client]
      ,[code_aux]
      ,[ndossier]
      ,[NOM]
      ,[NOM_PROMOTION]
      ,[Montant]
      ,[Taux]
      ,[Remise]
      ,[Remise_supp]
      ,[Total_Remise]
      ,[etat]
      ,[observation]
      ,[date_dossier]
      ,[numAvoir]
      ,[date_avoir]
      ,[observation_avoir]
      ,[facts]
      ,[date_facts] 
      ,[Mois_Factures]
      ,[Commercial]
      ,[Superviseur]
      ,[Facture]
      ,[Date_Facturation]
      ,e.opperant Operant
from AF_VIEW_BORDEREAU_ALL b
LEFT JOIN [bmsql2].[DWH_GROUPE].[ventes].entetes_transaction e 
on (b.facture= e.num_trans and @entite=e.entite and e.type_trans='FA' and e.annee_trans=YEAR(date_facturation))

WHERE (DATE_DOSSIER &gt;= @dated ) and (DATE_DOSSIER&lt;=@datef)
AND code_client in (@client)
and nom_promotion in (@promotion)
and (etat=@etat OR @etat='TOUT')
ORDER BY date_dossier desc,ID_GROUPE_FACT desc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
