  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSourceExpFinance</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@entite">
            <Value>=Parameters!entite.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dated">
            <Value>=Parameters!dated.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@datef">
            <Value>=Parameters!datef.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>Select 
af.numAvoir,af.dateAvoir,af.montant,af.numAvoirComptable,af.designation,af.montantCheque,af.montantCreance,convert(varchar(255),af.Observation) as Observation,af.typeAvoir
,c.NOM NomClient,c.code_clien,c.code_aux, 
ag.Libre,
isnull(dbo.FN_CONCAT_FACT(b.ID_GROUPE_FACT),
											ISNULL(dbo.FN_CONCAT_FACT_LIBRE(agf.ID_GROUPE_FACT),'Exercice: ' + convert(varchar(20),Year(agf.DATE_CADEAU)))
) as facts
,isnull(dbo.FN_CONCAT_DATE_FACT(b.ID_GROUPE_FACT),
											ISNULL(dbo.FN_CONCAT_FACT_DATE_LIBRE(agf.ID_GROUPE_FACT),'Exercice: ' + convert(varchar(20),Year(agf.DATE_CADEAU)))
) as date_facts
,isnull(b.Montant,agf.MNT_HT) Montant_cmd
,isnull(b.NOM_PROMOTION,'AVOIR LIBRE') NOM_PROMOTION
,isnull(b.Taux,0) Taux
,isnull(b.Remise_supp,0) Remise_supp
,isnull(b.Remise,0) Remise
,isnull(b.Total_Remise,0) Total_Remise
FROM AVOIR_FINANCIER af
join AF_AVOIR_GROUPE ag on af.numAvoir=ag.numAvoir
left join GROUPE_FACTURE gf on ag.IDG=gf.ID_GROUPE_FACT and ag.Libre=0
left join AF_GROUPE_FACTURE agf on ag.IDG=agf.ID_GROUPE_FACT and (ag.Libre=1 or ag.libre=2)
left join AF_BORDEREAU b on b.ID_GROUPE_FACT=ag.IDG and ag.Libre=0
left join AF_CLIENT c on ISNULL(gf.CODE_CLIENT,agf.CODE_CLIENT)=c.code_clien and c.entite=@entite
where ag.Libre !=3
AND (dateAvoir between @dated and @datef)


UNION 

Select 
af.numAvoir,af.dateAvoir,af.montant,af.numAvoirComptable,af.designation,af.montantCheque,af.montantCreance,convert(varchar(255),af.Observation) as Observation,af.typeAvoir
,c.NOM NomClient,c.code_clien,c.code_aux, 
ag.Libre
,dbo.FN_CONCAT_FACT_EF_LIBRE(agf.ID_GROUPE_FACT)as facts
,dbo.FN_CONCAT_FACT_EF_DATE_LIBRE(agf.ID_GROUPE_FACT)as date_facts
,agf.MNT_HT Montant_cmd
,'ESCOMPTE FINANCIER' NOM_PROMOTION
,p.Percentage Taux
,0 Remise_supp
,0 Remise
,p.Percentage * agf.MNT_HT /100 Total_Remise
FROM AVOIR_FINANCIER af
join AF_AVOIR_GROUPE ag on af.numAvoir=ag.numAvoir
left join AF_EF_GROUPE_FACTURE agf on ag.IDG=agf.ID_GROUPE_FACT and (ag.libre=3)
left join AF_CLIENT c on agf.CODE_CLIENT=c.code_clien and c.entite=@entite
left join AF_PALIER_EF p on agf.NB_CADEAU=p.idPalier
where ag.Libre =3
AND (dateAvoir between @dated and @datef)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
